#new-reports-edit
  .col-xs-6.new-reports-content
    = form_for @bibliography do |f|
      %h2
        新しい参考文献
      .form-body
        .form-group
          = f.label :name, "タイトル", class: "control-label"
          = f.text_field :name, class: "form-control"
        .form-group
          = f.label :url, "URL", class: "control-label"
          = f.text_field :url, class: "form-control"
        .form-group
          = f.label :report_id, "report_id", class: "control-label"
          = f.number_field :report_id, class: "form-control"
        = f.submit "送信する", class: "btn btn-success"
        // = link_to "Back", :back